{
  "version": 2,
  "builds": [
    {
      "src": "./index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "functions": {
    "api/**/*.py": {
      "excludeFiles": "public/**",
      "memory": 128,
      "maxDuration": 10
    }
  }
}
